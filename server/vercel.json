{
  "name": "roommate-dhoondho-backend",
  "version": 2,
  "views": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "views/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/views/$1"
    }
  ]
} 